@font-face
  font-family: 'FontAwesome'
  src: url('./fonts/fa-solid-900.eot')
  src: url('./fonts/fa-solid-900.eot?#iefix') format('embedded-opentype'),
  url('./fonts/fa-regular-400.woff') format('woff'),
  url('./fonts/fa-regular-400.ttf') format('truetype')
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Myriad Pro Regular'
  src: url('./fonts/MyriadPro-Regular.eot')
  src: url('./fonts/MyriadPro-Regular.eot?#iefix') format('embedded-opentype'),
  url('./fonts/MyriadPro-Regular.woff') format('woff'),
  url('./fonts/MyriadPro-Regular.ttf') format('truetype')
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Myriad Pro Bold'
  src: url('./fonts/MyriadPro-Bold.eot')
  src: url('./fonts/MyriadPro-Bold.eot?#iefix') format('embedded-opentype'),
  url('./fonts/MyriadPro-Bold.woff') format('woff'),
  url('./fonts/MyriadPro-Bold.ttf') format('truetype')
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Myriad Pro Light SemiExtended'
  src: url('./fonts/MyriadPro-LightSemiExt.eot')
  src: url('./fonts/MyriadPro-LightSemiExt.eot?#iefix') format('embedded-opentype'),
  url('./fonts/MyriadPro-LightSemiExt.woff') format('woff'),
  url('./fonts/MyriadPro-LightSemiExt.ttf') format('truetype')
  font-weight: normal
  font-style: normal

body
 background-color: #ffffff
 color: #000000
 font-family: 'Myriad Pro Regular'
 font-size: 14px
 max-width:1920px
 min-width: 600px

.header
  text-align: center

.header__round
  background: #ffffff
  border: 1px solid #3d6cb0
  border-radius: 50%
  position: relative
  margin: 0 auto

.header__round::after
  content:""
  display: block
  position: absolute
  background:#3d6cb0
  border-radius: 50%
  top: 10px
  left: 10px

.header__name
  font-size: 54px
.header__surname
  font-size: 64px
  margin-left: -1px
.header__profession
  font-size: 25.78px
.about__header
  font-size: 42.47px
.about__info
  font-size: 15.92px
span.info__bold
  font-size: 15.92px
  line-height: 17px
.contact__header
  font-size: 21.23px
  color: #3d6cb0
.contact__data
  font-size: 21.23px
.info__header
  font-size: 26.54px
  text-transform: uppercase
.experience__description
  font-size: 15.92px
.experience__period
  font-size: 26.54px
.header__round
  width: 238px
  height: 238px
.header__round::after
  width: 208px
  height: 208px
  top: 15px
  left: 15px
@media (max-width:1366px)
  .col-1
    width:555px
  .col-2
    width:772px

// @media (max-width: 1280px)
@media (max-width: 1362px)
  .col-1
    width: 48%
    float: left
  .col-2
    width: 48%
    float: right
  .about
    width: 100%
  .content
    width: 100%
  table
    margin: 0 !important
    padding: 0!important

@media (max-width: 900px)
  .col-1
    width: auto
    display: inline-block
    margin: 0 auto
    padding: 0
  .col-2
    width: auto
    display: inline-block
    margin: 0 auto
    padding: 0
  .header__line
    width: 50%
  .col-1 .about
    margin: 0 !important
    padding: 0!important
  table
    margin: 0 !important
    padding: 0!important
  .about
    width: 100%
    margin: 0 auto
    padding: 0
  .content
    width: 100%
    padding: 0
    margin: 0 auto
  .col-2 .content
    padding-right: 0!important

@media (max-width: 700px)
  .col-1
    width: auto
    display: inline-block
    margin: 0 auto
    padding: 0
  .col-2 .content
    padding-right: 0!important
  .col-2
    width: auto
    display: inline-block
    margin: 0 auto
    padding: 0
  ul,
  li,
  .experience__description
    width: 100%
  .col-1 .about,
  .profile
    margin: 0
    padding: 0
  .about
    width: 100%
    margin: 0 auto
    padding: 0
  .content
    width: 100%
    padding: 0
    margin: 0 auto
  .header__round
    width: 120px
    height: 120px
  .header__round::after
    width: 100px
    height: 100px
    top: 10px
    left: 10px
  .header__name
    font-size: 50px
  .header__surname
    font-size: 60px
  .header__profession
    font-size: 22px
  .about__header
    font-size: 38px
  .about__info
    font-size: 13px
  span.info__bold
    font-size: 13px
    line-height: 14px
  .contact__header
    font-size: 18px
    color: #3d6cb0
  .contact__data
    font-size: 18px
  .info__header
    font-size: 23px
    text-transform: uppercase
  .experience__description
    font-size: 13px
  .experience__period
    font-size: 23px
  .content
    width: auto
  table
    margin: 0 !important
    padding: 0!important
/* start margin-top for headers*/
