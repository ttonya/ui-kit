.experience,
.software
  margin-top: 41px
/* end margin-top for headers*/

/*start icons*/
.profile__icon
  background: url('../img/icons.png') no-repeat -56px 3px
.contact__icon
  background: url('../img/icons.png') no-repeat -118px 3px
.skills__icon
  background: url('../img/icons.png') no-repeat -181px 3px
.education__icon
  background: url('../img/icons.png') no-repeat 5px 3px
.experience__icon
  background: url('../img/icons.png') no-repeat -305px 3px
.software__icon
  background: url('../img/icons.png') no-repeat -243px 3px
